import <%= props.name %> from "#<%= props.name.toLowerCase() %>/domain/entities/<%= props.name.toLowerCase() %>";

export type <%= props.name %>Output = {
  id: string;
  created_at: Date;<% if(!props.options.withoutUpdate){ %>
  updated_at: Date;<%}%>
};

export class <%= props.name %>OutputMapper {
  static toOutput(entity: <%= props.name %>): <%= props.name %>Output {
    return entity.toJSON();
  }
}
